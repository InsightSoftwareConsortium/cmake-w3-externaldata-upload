{
  "name": "cmake-w3-externaldata-upload",
  "version": "3.0.0",
  "scripts": {
    "start": "concurrently -n \"hypha,vite\" --success first -k \"pnpm start-hypha\" \"vite build --sourcemap true --watch\"",
    "dev": "pnpm start",
    "build": "VITE_HYPHA_SERVER_URL=https://hypha.aicell.io vite build",
    "start-hypha": "VITE_HYPHA_SERVER_URL=http://localhost:9000 PYTHONPATH=./src hypha --host=0.0.0.0 --port=9000 --startup-functions=./src/hypha-startup-functions.py:hypha_startup --static-mounts=/app/:./dist/",
    "start-hypha-service": "VITE_HYPHA_SERVER_URL=https://hypha.aicell.io -e PYTHONPATH=./src python ./src/register-hypha-uploader-service.py",
    "preview": "VITE_HYPHA_SERVER_URL=https://hypha.aicell.io vite preview --port 4173"
  },
  "dependencies": {
    "@ipld/car": "^5.2.6",
    "@ucanto/core": "^9.0.1",
    "@ucanto/principal": "^9.0.0",
    "@web3-storage/w3up-client": "^12.0.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "files-from-path": "^1.0.4",
    "hypha-rpc": "0.20.53-post-1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vite": "^6.3.5"
  }
}
